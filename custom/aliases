# medumount - for mounting remote hard drives from medusa, requires fuse for os x
alias medumount="sshfs medusa:/home/sma /Users/sma/sshfs/medusa/home/sma; sshfs medusa:/homee/sma /Users/sma/sshfs/medusa/homee/sma; sshfs medusa:/homeib/sma /Users/sma/sshfs/medusa/homeib/sma"

# namback - rsync command to back up my user data to my server
alias namback="rsync -az --delete --exclude-from '.rsync_exclude' Documents namira:backup/; rsync -az --delete --exclude-from '.rsync_exclude' Games namira:backup/; rsync -az --delete --exclude-from '.rsync_exclude' Movies namira:backup/; rsync -az --delete --exclude-from '.rsync_exclude' Music namira:backup/; rsync -az --delete --exclude-from '.rsync_exclude' Parallels namira:backup/; rsync -az --delete --exclude-from '.rsync_exclude' Pictures namira:backup/; rsync -az --delete --exclude-from '.rsync_exclude' Developer namira:backup/"
