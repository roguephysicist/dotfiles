# plotex - for creating plots in pdf from gnuplot epslatex files
function plotex() {
	local tex=`grep "output" $1 | awk -F\" '{print $(NF-1)}'`
	local name=`basename $tex .tex`
	gnuplot $1
	epstopdf $name-inc.eps
	pdflatex $tex
	rm $name-inc.eps
	rm $name-inc.pdf
	rm $name.aux
	rm $name.log
	rm $tex
	clear
}
